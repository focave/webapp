        "autoprefixer": "^10.4.2",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^3.2.1",
        "eslint-plugin-tailwindcss": "^3.5.0",
        "postcss": "^8.4.5",
        "postcss-load-config": "^3.1.1",
        "prettier": "^2.5.1",
